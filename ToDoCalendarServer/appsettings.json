{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Port=5432;Database=CalendarDatabase;Username=postgres;Password=root;"
  },
  "SmtpConfiguration": {
    "Topic": "Registration confirmation",
    "Host": "smtp.gmail.com",
    "Port": 465,
    "FromAdress": "parahinkv@gmail.com",
    "FromPassword": "lixs zzet saoq ftsm "
  },
  "NotificationConfiguration": {
    "IterationDelay": "00:01:00",
    "ReminderOffset": "00:10:00"
  },
  "RootConfiguration": {
    "RootPassword": "root1234",
    "MinimalAccountAge": 14
  },
  "AdsPush": {
    "MyApp": {
      "TargetMappings": {
        "Android": "FirebaseCloudMessaging"
      },
      "FirebaseCloudMessaging": {
        "Type": "<type filed in service_account.json>",
        "ProjectId": "<project_id filed in service_account.json>",
        "PrivateKey": "<private_key filed in service_account.json>",
        "PrivateKeyId": "<private_key_id filed in service_account.json>",
        "ClientId": "<client_id filed in service_account.json>",
        "ClientEmail": "<client_email filed in service_account.json>",
        "AuthUri": "<auth_uri filed in service_account.json>",
        "AuthProviderX509CertUrl": "<auth_provider_x509_cert_url filed in service_account.json>",
        "TokenUri": "<client_x509_cert_url filed in service_account.json>",
        "ClientX509CertUrl": "<token_uri filed in service_account.json>"
      }
    },
  },
    "Serilog": {
      "Using": [ "Serilog.Sinks.File" ],
      "MinimumLevel": {
        "Default": "Information"
      },
      "WriteTo": [
        {
          "Name": "Console",
          "Args": {
            "outputTemplate": "{Timestamp:o} [{Level}] ({Application}/{MachineName}/{ThreadId}) {SourceContext:l} {Scope} {Message}{OneLinerException}{NewLine}"
          }
        }
      ],
      "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId", "WithOneLinerExceptions" ]
    }
  }
